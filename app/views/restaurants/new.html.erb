<h1>Restaurants</h1>
<p>Add a New Restaurant!</p>


<%= simple_form_for(@restaurant) do |f| %>
  <%= f.error_notification %>
  <%= f.input :name %>
  <%= f.input :category, collection: ["Chinese","Italian","Japanese","French","Belgian", "Korean"] %>
  <%= f.input :address %>
  <%= f.input :phone_number %>
  <%= f.button :submit %>
<% end %>
